{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "build",
        "type": "shell",
        "command": "g++",
        "args": [
          "-ID:/IBM/ILOG/CPLEX_Studio201/cplex/include",
          "-ID:/IBM/ILOG/CPLEX_Studio201/concert/include",
          "${file}",
          "-o",
          "${fileDirname}/${fileBasenameNoExtension}.exe",
          "-LD:/IBM/ILOG/CPLEX_Studio201/cplex/lib/x64_windows_msvc14/stat_mda",
          "-LD:/IBM/ILOG/CPLEX_Studio201/concert/lib/x64_windows_msvc14/stat_mda",
          "-lilocplex",
          "-lconcert",
          "-lcplex2010",
          "-lm",
          "-lpthread"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "problemMatcher": ["$gcc"],
        "detail": "compile C++ files and link CPLEX libraries"
      }
    ]
  }
  